{
  "name": "jycraft-codecraft",
  "version": "0.0.1",
  "description": "Web-based coding, sharing, and distribution environment for Jycraft",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.build.js --bail -p",
    "dev": "node node_modules/.bin/webpack-dev-server --history-api-fallback --hot --inline --progress",
    "test:midway": "karma start",
    "test:midway:live": "karma start --auto-watch --no-single-run",
    "test:unit": "mocha --compilers babel:babel/register src/**/*.specs.js",
    "test": "mocha --compilers babel:babel/register src/**/*.specs.js"
  },
